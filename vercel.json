{
  "version": 2,
  "buildCommand": "CI=false npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ]
} 